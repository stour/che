[
    {
        "id": "java-default",
        "creator": "ide",
        "name": "Java",
        "description": "Default Java Stack with JDK 8, Maven and Tomcat.",
        "scope": "general",
        "tags": [
            "Java",
            "JDK",
            "Maven",
            "Tomcat",
            "Subversion",
            "Ubuntu",
            "Git"
        ],
        "components": [
            {
                "name": "JDK",
                "version": "1.8.0_45"
            },
            {
                "name": "Maven",
                "version": "3.2.2"
            },
            {
                "name": "Tomcat",
                "version": "8.0.24"
            }
        ],
        "source": {
            "type": "image",
            "origin": "codenvy/ubuntu_jdk8"
        },
        "workspaceConfig": {
            "environments": [
                {
                    "name": "default",
                    "recipe": null,
                    "machineConfigs": [
                        {
                            "name": "default",
                            "limits": {
                                "ram": 1000
                            },
                            "source": {
                                "location": "stub",
                                "type": "recipe"
                            },
                            "type": "docker",
                            "dev": true
                        }
                    ]
                }
            ],
            "name": "default",
            "defaultEnv": "default",
            "description": null,
            "commands": [
                {
                    "commandLine": "mvn clean install -f ${current.project.path}",
                    "name": "newMaven",
                    "type": "mvn"
                }
            ]
        },
        "permissions": {
            "groups": [
                {
                    "acl": [
                        "read",
                        "search"
                    ],
                    "name": "public"
                }
            ],
            "users": {}
        },
        "stackIcon" : {
            "name": "type-java.svg",
            "mediaType": "image/svg+xml"
        }
    },
